<launch>
  <arg name="gui" default="false"/>
  <include file="$(find sr_robot_launch)/launch/sr_right_ur10arm_hand.launch">
    <arg name="eth_port" value="eth0" />
    <arg name="hand_serial" value="1546" />
    <arg name="use_moveit" value="true"/>
    <arg name="gui" value="$(arg gui)"/>
    <arg name="robot_description" value="$(find sr_multi_description)/urdf/right_srhand_lite_ur10_joint_limited.urdf.xacro" />
    <arg name="robot_config_file" value="$(find sr_multi_moveit_config)/config/robot_configs/right_sh_lite_ur10.yaml" />
    <arg name="mapping_path" value="$(find sr_edc_launch)/mappings/default_mappings/rh_lite_v1.yaml"/>
    <arg name="sim" value="false"/>
  </include>
</launch>