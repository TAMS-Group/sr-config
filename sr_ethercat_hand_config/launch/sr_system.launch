<launch>
  <include file="$(find sr_ethercat_hand_config)/launch/sr_rosbag_log.launch">
    <arg name="log_bag_prefix" value="sr_system"/>
  </include>
  <arg name="sim" default="false"/>
  <arg name="gui" default="true"/>
  <arg name="scene" default="false"/>
  <arg name="eth_port" default="enp1s0"/>
  
  <include file="$(find sr_robot_launch)/launch/sr_right_ur10arm_hand.launch">
    <arg name="eth_port" value="$(arg eth_port)"/>
    <arg name="hand_serial" value="1818" />
    <arg name="scene" value="$(arg scene)"/>
    <arg name="robot_description" value="'$(find sr_multi_description)/urdf/right_srhand_ur10_bt_sp_biotacs.urdf.xacro'"/>
    <arg name="use_moveit" value="true"/>
    <arg name="gui" value="$(arg gui)"/>
    <arg name="mapping_path" value="$(find sr_edc_launch)/mappings/default_mappings/rh_E_v3.yaml"/>
    <arg name="sim" value="$(arg sim)"/>
    <arg name="robot_config_file" value="$(find sr_multi_moveit_config)/config/robot_configs/right_sh_ur10_btsp_biotac.yaml"/>
    <arg name="arm_robot_hw" default="$(find sr_ur_arm_config)/config/ur_arm_1_robot_hw.yaml"/>
  </include>
</launch>
