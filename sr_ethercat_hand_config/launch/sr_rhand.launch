<launch>
  <include file="$(find sr_logging_common)/launch/sr_rosbag_log.launch">
    <arg name="log_bag_prefix" value="sr_rhand"/>
  </include>
  <include file="$(find sr_robot_launch)/launch/srhand.launch">
    <arg name="eth_port" value="enp0s31f6" />
    <arg name="hand_serial" value="922" />
    <arg name="hand_id" value="rh" />

    <arg name="robot_description" value="$(find sr_description)/robots/sr_three_finger_rf_motor.urdf.xacro" />
    <arg name="mapping_path" value="$(find sr_edc_launch)/mappings/default_mappings/rh_E_v1.yaml"/>

    <arg name="use_moveit" value="true"/>
    <arg name="sim" value="false"/>
  </include>
</launch>
