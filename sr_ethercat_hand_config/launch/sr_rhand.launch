<launch>
  <include file="$(find sr_robot_launch)/launch/sr_right_ur5arm_hand.launch">
    <arg name="eth_port" value="eth0" />
    <arg name="hand_serial" value="1066"/>
    <arg name="hand_id" value="rh" />
    <arg name="robot_description" value="$(find sr_multi_description)/urdf/right_srhand_extra_lite_ur5_joint_limited.urdf.xacro"/>
    <arg name="robot_config_file" value="$(find sr_multi_moveit_config)/config/robot_configs/right_sh_extra_lite_ur5.yaml"/>
    <arg name="mapping_path" value="$(find sr_edc_launch)/mappings/default_mappings/rh_extra_lite_v1.yaml"/>

    <arg name="sim" value="false"/>
    <arg name="use_moveit" value="true"/>
  </include>
</launch>
